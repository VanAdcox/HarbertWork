@page "/bmi"
@rendermode InteractiveServer

<PageTitle>"BMI"</PageTitle>

<h1>BMI</h1>
<h2>Height</h2>
<h3>Feet</h3>
<input @onchange="feetChange">
<h3>Inches</h3>

@code {
	int feet;
	int inches;

	private void feetChange(ChangeEventArgs e) => feet =  Int32.Parse(e.Value!);
	private void inchesChange(ChangeEventArgs e) => inches =  Int32.Parse(e.Value!);

}
